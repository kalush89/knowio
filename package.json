{
  "name": "knowio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "tsx src/test/run-e2e-tests.ts",
    "test:e2e:watch": "vitest src/test/e2e-integration.test.ts src/test/real-docs-integration.test.ts src/test/performance-integration.test.ts",
    "test:integration": "vitest --run src/**/*.integration.test.ts",
    "test:unit": "vitest --run src/**/*.test.ts --exclude src/**/*.integration.test.ts --exclude src/test/*.test.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:migrate:status": "prisma migrate status",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:prod": "NODE_ENV=production tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "deploy": "./scripts/deploy.sh",
    "deploy:prod": "./scripts/deploy.sh production",
    "health-check": "curl -f http://localhost:3000/api/monitoring/health || exit 1",
    "validate-env": "node scripts/validate-environment.js",
    "db:reset": "prisma migrate reset --force",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-bedrock": "^3.865.0",
    "@aws-sdk/client-bedrock-runtime": "^3.864.0",
    "@prisma/client": "^6.14.0",
    "bcrypt": "^6.0.0",
    "inngest": "^3.40.1",
    "next": "15.4.6",
    "next-auth": "^5.0.0-beta.29",
    "playwright": "^1.54.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.0.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.19.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "prisma": "^6.14.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
